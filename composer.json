{
    "name": "uuf6429/git-project-control",
    "description": "Set up and manage contribution policies for your PHP-based projects.",
    "homepage": "https://github.com/uuf6429/git-project-control",
    "type": "application",
    "license": "MIT",
    "authors": [
        {
            "name": "Christian Sciberras",
            "email": "christian@sciberras.me"
        }
    ],
    "support": {
        "issues": "https://github.com/uuf6429/git-project-control/issues"
    },
    "require": {
        "php": ">=5.5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "friendsofphp/php-cs-fixer": "^1.11"
    },
    "autoload": {
        "psr-4": {
            "uuf6429\\GitProjectControl\\": "src/GitProjectControl"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "uuf6429\\GitProjectControl\\": "test/GitProjectControl"
        }
    },
    "suggest": {
        "friendsofphp/php-cs-fixer": "Needed for PhpCsFixerAction"
    },
    "bin": ["git-project-control"]
}
